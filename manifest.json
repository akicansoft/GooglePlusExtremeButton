{
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "content_scripts": [
    {
      "exclude_globs": [
      ],
      "include_globs": [
        "https://plus.google.com/u/*/",
        "https://plus.google.com/",
        "https://plus.google.com",
        "https://plus.google.com/?hl=ja&partnerid=gplp0",
        "https://plus.google.com/stream/circles/*",
        "https://plus.google.com/communities/*",
        "https://plus.google.com/explore",
        "https://plus.google.com/*/posts",
        "https://plus.google.com/events/*",
        "https://plus.google.com?hl=ja",
        "https://plus.google.com/?hl=ja"
      ],
      "js": [
        "scripts/gpeb.js"
      ],
      "matches": [
        "http://plus.google.com/*",
        "https://plus.google.com/*",
        "https://www.evernote.com/*",
        "http://www.evernote.com/*"
      ],
      "permissions": [
        "http://readitlaterlist.com/*",
        "https://www.evernote.com/*",
        "http://www.evernote.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "converted_from_user_script": true,
  "default_locale": "ja",
  "description": "__MSG_extensiondescription__",
  "icons": {
    "128": "images/icon.png"
  },
  "manifest_version": 2,
  "name": "Google+ Extreme Button",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "3.0.1",
  "web_accessible_resources": [
    "buttons/Aisatsu.png",
    "buttons/CircleCount.png",
    "buttons/Delicious.png",
    "buttons/Dokoina.png",
    "buttons/Evernote.png",
    "buttons/Facebook.png",
    "buttons/Facebook.png",
    "buttons/FullSizeImgDown.png",
    "buttons/FullSizeImgOpen.png",
    "buttons/fuu.png",
    "buttons/gununu.png",
    "buttons/Hatena.png",
    "buttons/icon.png",
    "buttons/mute.png",
    "buttons/oh.png",
    "buttons/Pinterest.png",
    "buttons/RIL1.png",
    "buttons/RIL2.png",
    "buttons/Ripples.png",
    "buttons/rtm.png",
    "buttons/Tumblr.png",
    "buttons/Twitter.png",
    "buttons/mini.png",
    "buttons/white.png",
    "images/icon.png",
    "buttons/settings.png"
  ]
}