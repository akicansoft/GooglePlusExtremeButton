{
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "content_scripts": [
    {
      "exclude_globs": [
      ],
      "include_globs": [
        "https://plus.google.com/u/*/",
        "https://plus.google.com/",
        "https://plus.google.com",
        "https://plus.google.com/?hl=ja&partnerid=gplp0",
